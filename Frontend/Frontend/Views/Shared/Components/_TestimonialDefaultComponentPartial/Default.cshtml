@model List<JobEntry.DTO.TestimonialDTOs.ResultTestimonialDto> 
<!-- Testimonial Start -->
        <div class="container-xxl py-5 wow fadeInUp" data-wow-delay="0.1s">
            <div class="container">
                <h1 class="text-center mb-5">Iste Musterilerimizin Dedikleri</h1>
                <div class="owl-carousel testimonial-carousel">
                    @foreach (var item in Model)
                    {
                        <div class="testimonial-item bg-light rounded p-4">
                            <i class="fa fa-quote-left fa-2x text-primary mb-3"></i>
                            <p>@item.Descrption</p>
                            <div class="d-flex align-items-center">
                                <img class="img-fluid flex-shrink-0 rounded" src="@item.ImageUrl" style="width: 50px; height: 50px;">
                                <div class="ps-3">
                                    <h5 class="mb-1">@item.Name</h5>
                                    <small>@item.Title</small>
                                </div>
                            </div>
                        </div>
                    }
                  
                </div>
            </div>
        </div>
        <!-- Testimonial End -->
        